<h1>All Projects</h1>
<% @projects.each do |project| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to project.title, project %>
      </h4>
        <small>
        submitted <%= time_ago_in_words(project.created_at) %> ago by <%= project.user.name %>
      </small>
    </div>
  </div>
<% end %>
